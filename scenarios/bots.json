{
  "name": "Bots Scorer",
  "steps": [
    {
      "id": "whatDoYouWantToDo",
      "type": "prompt",
      "data": { "text": "What do you want to do?" }
    },
    {
      "id": "userIntent",
      "type": "score",
      "data": { 
        "source": "whatDoYouWantToDo", 
        "bots": [ "bot-common-responses", "Bottle" ]
      },
      "scenarios": [
        {
          "condition": "userIntent == 'set_alarm'",
          "steps": [
            {
              "type": "handler",
              "data": { "name": "set_alarm" }
            }
          ]
        },
        {
          "condition": "userIntent == 'quit'",
          "steps": [
            {
              "type": "exit"
            }
          ]
        }
      ]
    }
  ],
  "bots": [
    {
      "name": "bot-common-responses",
